{
    "path": {
        "src": {
            "srcPath": "./src",
            "components": "/_components",
            "html": [
                "/view/**/*.html"
            ],
            "style": [
                "/assets/css/styles.scss"
            ],
            "script": [
                "/assets/js/app.js"
            ],
            "image": [
                "/assets/img/**/*.*"
            ],
            "font": [
                "/assets/fonts/**/*.*"
            ],
            "libs": [
                "/assets/libs/**/*.*"
            ]
        },
        "dist": {
            "distPath": "./dist",
            "html": "/",
            "style": "/assets/css/",
            "script": "/assets/js/",
            "image": "/assets/img/",
            "font": "/assets/fonts/",
            "libs": "/assets/libs/"
        }
    }
}